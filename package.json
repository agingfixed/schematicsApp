{
  "name": "schematicsapp",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "node -e \"fs.rmSync('build-tests',{recursive:true,force:true});\" && tsc --project tsconfig.tests.json && node scripts/fix-esm-extensions.mjs && node --test build-tests/utils/__tests__/*.js",
    "preview": "vite preview",
    "desktop:dev": "npm --prefix apps/desktop run dev",
    "desktop:start": "npm --prefix apps/desktop run start",
    "desktop:build": "npm --prefix apps/desktop run build",
    "desktop:package": "npm --prefix apps/desktop run package",
    "desktop:release": "node scripts/desktop-release.mjs",
    "postinstall": "npm --prefix apps/desktop install"
  },
  "dependencies": {
    "nanoid": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.2.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11"
  }
}
